    font-size: 28px;
    -unity-font-style: bold;
    color: rgb(55, 41, 30);
    -unity-text-align: upper-center;
    height: auto;
    padding-top: 0;
    padding-right: 4px;
    padding-bottom: 6px;
    padding-left: 4px;
    margin-bottom: 4px;
    flex-grow: 1;
}

.level-1 {
