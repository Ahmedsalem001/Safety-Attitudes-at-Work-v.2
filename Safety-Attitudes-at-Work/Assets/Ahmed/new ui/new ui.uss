    -unity-background-image-tint-color: rgba(214, 214, 214, 0.64);
}

.showLoadMenu {
    flex-grow: 1;
    position: absolute;
    width: 1373px;
    height: 893px;
    background-image: url('project://database/Assets/Ahmed/new%20ui/menu%201.png?fileID=21300000&guid=e7d33fa7cdb899344856ecdff6345e6f&type=3#menu 1');
    top: 77px;
    left: 288px;
    transition-timing-function: ease-out-elastic;
    display: flex;
    opacity: 1;
    transition-duration: 0.3s;
    transition-property: opacity;
    transition-delay: 0.2s;
}

.hideLoadMenu {
    display: none;
    opacity: 0;
}

.unity-base-field {
    width: 500px;
    align-items: center;
    justify-content: center;
    align-self: auto;
    opacity: 1;
}

