}

.btn {
    background-image: url('project://database/Assets/Ahmed/new%20ui/Achtung-Cthulhu-Tactics12172020-010221-56044.png?fileID=21300000&guid=0f959d8b6ef518a468ad1d1fa70ef055&type=3#Achtung-Cthulhu-Tactics12172020-010221-56044');
    -unity-background-image-tint-color: rgb(255, 255, 255);
    width: 400px;
    height: 70px;
