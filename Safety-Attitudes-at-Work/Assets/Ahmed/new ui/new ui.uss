.btn:hover {
    scale: 1.1 1.1;
    transition-timing-function: ease-out;
    transition-duration: 0.5s;
}

.btn:active {
    -unity-background-image-tint-color: rgba(200, 195, 195, 0.36);
}

.fontStyle {
